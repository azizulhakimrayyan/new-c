#include <stdio.h>
int main()
{
  int age = 22;
  int *ptr = &age;
  // address
  printf("%p\n", &age);
  printf("%u\n", &age);
  printf("%d\n", &age);
  printf("%s\n", "----");
  printf("%p\n", ptr);
  printf("%u\n", ptr);
  printf("%d\n", ptr);
  printf("%s\n", "----");
  printf("%p\n", &ptr);
  printf("%u\n", &ptr);
  printf("%d\n", &ptr);
  return 0;
}
