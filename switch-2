#include <stdio.h>
int main()
{
  int day;
  printf("enter a day:(1-7)");
  scanf("%d", &day);
  switch (day)
  {
  case 1:
    printf("saturday");
    break;
  case 2:
    printf("sunday");
    break;
  case 3:
    printf("Monday");
    break;
  case 4:
    printf("tuesday");
    break;
  case 5:
    printf("wednesday");
    break;
  case 6:
    printf("thursday");
    break;
  case 7:
    printf("friday");
    break;
  default:
    printf("not a valid day \n please prass a valid day ");
  }

  return 0;
}
