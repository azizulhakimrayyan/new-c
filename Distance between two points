#include <stdio.h>
#include <math.h>
int main()
{
  float x1, x2, y1, y2;
  printf("Enter the co-ordinates (x1,y1)(like: 2 3 for (2,3)):");
  scanf("%f %f", &x1, &y1);
  printf("Enter the co-ordinates (x2,y2)(like: 2 3 for (2,3)):");
  scanf("%f %f", &x2, &y2);
  printf("Distance between two points:%.3f", pow(pow(x2 - x1, 2) + pow(y2 - y2, 2), 0.5));
  return 0;
}
