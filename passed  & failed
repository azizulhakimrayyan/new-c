#include <stdio.h>
int main()
{
  int marks;
  printf("enter your marks:(0 to 100)");
  scanf("%d", &marks);
  if (marks >= 30 && marks <= 100)
  {
    printf("passed");
  }
  else if (marks > 100)
  {
    printf(" enter a valid number(0 to 100)");
  }

  else
  {
    printf("failed");
  }

  return 0;
}
